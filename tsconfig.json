{
  "compilerOptions": {
    "target": "ES2022",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "types": [
      "node",
      "vite/client",
      "google.maps"
    ],
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "moduleDetection": "force",
    "allowJs": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components": [
        "./components/index.ts"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@pages": [
        "./pages/index.ts"
      ],
      "@pages/*": [
        "./pages/*"
      ]
    },
    "noEmit": true
  }
}